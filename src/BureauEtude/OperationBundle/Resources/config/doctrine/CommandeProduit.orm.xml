<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BureauEtude\CoreBundle\Entity\CommandeProduit" table="commande_produit">
    <indexes>
      <index name="FK_commande_produit_ISBN" columns="ISBN"/>
      <index name="FK_commande_produit_id_etat" columns="id_etat"/>
      <index name="IDX_DF1E9E873E314AE8" columns="id_commande"/>
    </indexes>
    <id name="idEtat" association-key="true"/>
    <id name="idCommande" association-key="true"/>
    <id name="isbn" association-key="true"/>
    <one-to-one field="isbn" target-entity="Produit" fetch="LAZY">
      <join-columns>
        <join-column name="ISBN" referenced-column-name="ISBN"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idCommande" target-entity="Commande" fetch="LAZY">
      <join-columns>
        <join-column name="id_commande" referenced-column-name="id_commande"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idEtat" target-entity="Etat" fetch="LAZY">
      <join-columns>
        <join-column name="id_etat" referenced-column-name="id_etat"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
