<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BureauEtude\CoreBundle\Entity\Commande" table="commande">
    <indexes>
      <index name="FK_commande_id_enfant" columns="id_enfant"/>
      <index name="FK_commande_numero_facture" columns="numero_facture"/>
    </indexes>
    <id name="idCommande" type="integer" column="id_commande">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateCommande" type="date" column="date_commande" nullable="false"/>
    <field name="montantCommande" type="float" column="montant_commande" precision="10" scale="0" nullable="false"/>
    <many-to-one field="idEnfant" target-entity="Enfant" fetch="LAZY">
      <join-columns>
        <join-column name="id_enfant" referenced-column-name="id_enfant"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="numeroFacture" target-entity="Facture" fetch="LAZY">
      <join-columns>
        <join-column name="numero_facture" referenced-column-name="numero_facture"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
