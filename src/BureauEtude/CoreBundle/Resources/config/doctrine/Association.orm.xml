<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BureauEtude\CoreBundle\Entity\Association" table="association">
    <indexes>
      <index name="FK_association_id_etablissement" columns="id_etablissement"/>
      <index name="FK_association_id_association_1" columns="id_association_1"/>
    </indexes>
    <id name="idAssociation" type="integer" column="id_association">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomAssociation" type="string" column="nom_association" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prixAdhesion" type="float" column="prix_adhesion" precision="10" scale="0" nullable="false"/>
    <many-to-one field="idAssociation1" target-entity="Association" fetch="LAZY">
      <join-columns>
        <join-column name="id_association_1" referenced-column-name="id_association"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEtablissement" target-entity="Etablissement" fetch="LAZY">
      <join-columns>
        <join-column name="id_etablissement" referenced-column-name="id_etablissement"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="idUtilisateur" target-entity="Utilisateur" inversed-by="idAssociation" fetch="LAZY">
      <join-table name="appartenir">
        <join-columns>
          <join-column name="id_association" referenced-column-name="id_association"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="id_utilisateur" referenced-column-name="id_utilisateur"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="idTuteur" target-entity="Tuteur" mapped-by="idAssociation" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
