<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BureauEtude\CoreBundle\Entity\Lister" table="lister">
    <indexes>
      <index name="FK_lister_id_etablissement" columns="id_etablissement"/>
      <index name="FK_lister_id_classe" columns="id_classe"/>
      <index name="FK_lister_id_discipline" columns="id_discipline"/>
      <index name="FK_lister_id_specialite" columns="id_specialite"/>
      <index name="IDX_D6253BB2FA891952" columns="ISBN"/>
    </indexes>
    <id name="idSpecialite" association-key="true"/>
    <id name="idClasse" association-key="true"/>
    <id name="idDiscipline" association-key="true"/>
    <id name="idEtablissement" association-key="true"/>
    <id name="isbn" association-key="true"/>
    <field name="annee" type="date" column="annee" nullable="true"/>
    <one-to-one field="isbn" target-entity="Produit" fetch="LAZY">
      <join-columns>
        <join-column name="ISBN" referenced-column-name="ISBN"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idEtablissement" target-entity="Etablissement" fetch="LAZY">
      <join-columns>
        <join-column name="id_etablissement" referenced-column-name="id_etablissement"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idDiscipline" target-entity="Discipline" fetch="LAZY">
      <join-columns>
        <join-column name="id_discipline" referenced-column-name="id_discipline"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idClasse" target-entity="Classe" fetch="LAZY">
      <join-columns>
        <join-column name="id_classe" referenced-column-name="id_classe"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idSpecialite" target-entity="Specialite" fetch="LAZY">
      <join-columns>
        <join-column name="id_specialite" referenced-column-name="id_specialite"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
