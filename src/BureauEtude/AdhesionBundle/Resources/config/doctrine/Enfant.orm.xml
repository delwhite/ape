<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BureauEtude\CoreBundle\Entity\Enfant" table="enfant">
    <indexes>
      <index name="FK_enfant_id_etablissement" columns="id_etablissement"/>
      <index name="FK_enfant_id_tuteur" columns="id_tuteur"/>
    </indexes>
    <id name="idEnfant" type="integer" column="id_enfant">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomEnfant" type="string" column="nom_enfant" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenomEnfant" type="string" column="prenom_enfant" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idEtablissement" target-entity="Etablissement" fetch="LAZY">
      <join-columns>
        <join-column name="id_etablissement" referenced-column-name="id_etablissement"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTuteur" target-entity="Tuteur" fetch="LAZY">
      <join-columns>
        <join-column name="id_tuteur" referenced-column-name="id_tuteur"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
