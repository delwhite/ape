<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BureauEtude\CoreBundle\Entity\Stock" table="stock">
    <indexes>
      <index name="FK_stock_id_etat" columns="id_etat"/>
      <index name="FK_stock_id_association" columns="id_association"/>
      <index name="IDX_4B365660FA891952" columns="ISBN"/>
    </indexes>
    <id name="idAssociation" association-key="true"/>
    <id name="idEtat" association-key="true"/>
    <id name="isbn" association-key="true"/>
    <field name="quantite" type="integer" column="quantite" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <one-to-one field="isbn" target-entity="Produit" fetch="LAZY">
      <join-columns>
        <join-column name="ISBN" referenced-column-name="ISBN"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idEtat" target-entity="Etat" fetch="LAZY">
      <join-columns>
        <join-column name="id_etat" referenced-column-name="id_etat"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idAssociation" target-entity="Association" fetch="LAZY">
      <join-columns>
        <join-column name="id_association" referenced-column-name="id_association"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
