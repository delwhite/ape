<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BureauEtude\CoreBundle\Entity\Fournisseur" table="fournisseur">
    <id name="idFournisseur" type="integer" column="id_fournisseur">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomFournisseur" type="string" column="nom_fournisseur" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresseFournisseur" type="string" column="adresse_fournisseur" length="300" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cpFournisseur" type="string" column="cp_fournisseur" length="5" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="villeFournisseur" type="string" column="ville_fournisseur" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
