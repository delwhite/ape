<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BureauEtude\CoreBundle\Entity\Produit" table="produit">
    <id name="isbn" type="string" column="ISBN" length="25">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titre" type="string" column="titre" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="auteur" type="string" column="auteur" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="editeur" type="string" column="editeur" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="anneeParution" type="date" column="annee_parution" nullable="false"/>
    <field name="reutilisable" type="boolean" column="reutilisable" nullable="false"/>
    <field name="quantiteMini" type="integer" column="quantite_mini" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
